<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <View
        android:layout_width="match_parent"
        android:layout_height="10dp" />

    <EditText
        android:id="@+id/create_habit_article_dialog_url"
        style="@style/EditTextTheme.Size"
        android:theme="@style/EditTextTheme"
        android:hint="@string/create_habit_youtube_article_url_hint"
        android:textColorHint="@color/md_white_1000"/>

    <EditText
        android:id="@+id/create_habit_article_dialog_name"
        style="@style/EditTextTheme.Size"
        android:theme="@style/EditTextTheme"
        android:hint="@string/create_habit_youtube_article_name_hint"
        android:textColorHint="@color/md_white_1000"/>


</LinearLayout>